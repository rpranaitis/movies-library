{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "camelcase": 0,
        "no-console": 0,
        "object-curly-newline": 0,
        "newline-per-chained-call": 0,
        "import/extensions": [
            {
                "js": 0
            }
          ]
    }
}
